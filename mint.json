    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.svg",

  "colors": {
    "primary": "#B22222",
    "light": "#FF7F7F",
    "dark": "#8B0000",
    "anchors": {
      "from": "#B22222",
      "to": "#FF6347"
    }
